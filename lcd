#include <LiquidCrystal.h>
const int rs = 12, en = 11, d4 = 5, d5 = 4, d6 = 3, d7 = 2;
LiquidCrystal lcd(rs, en, d4, d5, d6, d7);

void setup() {
  lcd.begin(20, 4);
  lcd.setCursor(0,0);
  lcd.print("ANY TIME MEDICINE");
  lcd.setCursor(0,1);
  lcd.print("1.FEVER 2.SUGAR");
  lcd.setCursor(0,2);
  lcd.print("3.BP 2.COLD");
}

void loop() {
  
}
